#include<stdio.h>
void main()
{
    int a,b;
    printf("Enter 2 numbers:");
    scanf("%d %d",&a,&b);
    if(a>b)
    {
        if(a%b==0)
        {
            printf("%d",a);
        }
        else
        {
            printf("%d",a*b);
        }
    }
    else
    { 
         if(b%a==0)
        {
            printf("%d",b);
        }
        else
        {
            printf("%d",a*b);
        }
        
    }
}
